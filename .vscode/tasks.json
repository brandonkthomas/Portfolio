{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build",
        "command": "dotnet",
        "type": "process",
        "args": [
          "build",
          "${workspaceFolder}/Portfolio.csproj",
          "/property:GenerateFullPaths=true",
          "/consoleloggerparameters:NoSummary"
        ],
        "problemMatcher": "$msCompile"
      },
      {
        "label": "dev:start",
        "type": "shell",
        "command": "/bin/bash",
        "args": [
          "-c",
          "bash \"${workspaceFolder}/.vscode/dev-run.sh\""
        ],
        "problemMatcher": []
      },
      {
        "label": "dev:stop",
        "type": "shell",
        "command": "/bin/bash",
        "args": [
          "-c",
          "bash \"${workspaceFolder}/.vscode/dev-stop.sh\""
        ],
        "problemMatcher": []
      }
    ]
}